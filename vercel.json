{
    "builds": [
      {
        "src": "inventory/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "inventory/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles/"
  }